{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "babel-plugin-styled-components",
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "_backgrounds": "./src/static/backgrounds",
          "_icons": "./src/static/icons",
          "_svg": "./src/static/svg",
          "_components": "./src/components",
          "_pages": "./src/pages"
        }
      }
    ]
  ]
}
